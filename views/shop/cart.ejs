<%- include('../partials/head.ejs') %>
    <link rel="stylesheet" href="/css/product.css">
</head>

    <body>
        <%- include('../partials/nav.ejs') %>
        <main>
            <% if(products.length>0) { %>
                <ul>
                    <% products.forEach(p=>{ %>
                        <li>
                            <%= p.title %>(<%= p.quantity%>) 
                             Total Price : Rs. <%= p.totPrice %>
                        </li>
                    <%})%>
                </ul>

            <%}else{%>
                <h1>No products Found</h1>
                <%}  %>
        </main>
        <%- include('../partials/footer.ejs') %>